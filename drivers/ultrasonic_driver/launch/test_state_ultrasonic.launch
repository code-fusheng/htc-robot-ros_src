<launch>

    <node pkg="state_machine" type="main.py" name="state_machine" output="screen">
        <rosparam command="load"  file="$(find state_machine)/params/smartcar.yaml"/>
    </node>

    <node pkg="ultrasonic_driver" type="main.py" name="ultrasonic_driver" output="screen">
        <param name="serial_port" value="/dev/ultrasonic_serial" />
        <param name="frame_id" value="ultrasonic_link" />
    </node>
    
</launch>

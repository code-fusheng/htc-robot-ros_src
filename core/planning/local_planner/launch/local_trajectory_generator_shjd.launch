<launch>
    <node pkg="local_planner" type="local_trajectory_generator_node" name="local_trajectory_generator_node" output="screen">
        <param name="horizonDistance" value="200"/>
        <param name="rollOutNumber" value="16"/>
        <param name="rollOutDensity" value="0.2"/>
        <param name="horizontalSafetyDistancel" value="0.3"/>
        <param name="verticalSafetyDistance" value="0.3"/>
        <param name="minFollowingDistance" value="50"/>
        <param name="lateralSkipDistance" value="6.0"/>
        <param name="width" value="1.3"/>
        <param name="length" value="2.2"/>
        <param name="wheel_base" value="1.18"/>
        
        <param name="lidar_frame" value="rslidar"/>
    </node>
</launch> 
